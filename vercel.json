{
  "name": "liqcalc-web-api",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["api-mock.json"]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://liqcalc-jobs.vercel.app"},
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS"}
      ]
    }
  ]
}